<div class="contents row">
  <div class="container">
    <%= form_for @user_review, :html => { multipart: true } do |f| %>
      <h3>
        Create a new logbook
      </h3>
      <div class="field">
        <%= f.label :Description %><br>
        <%= f.text_area :body %>
      </div>
      <ul>
        <li><%= file_field_tag "images[]", type: :file , multiple: true %></li>
      </ul>
      <%= f.submit 'Post'%>
    <% end %>
  </div>
</div>